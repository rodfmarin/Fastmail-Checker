{
    "background": {
        "persistent": false,
        "page": "background.html"
    },
    "content_scripts": [{
        "matches": ["*://fastmail.com/*"],
        "js": ["content.js"]
    }],
    "browser_action": {
        "default_icon": "fastmail_not_logged_in.png"
    },
    "description": "A Fastmail checker / unread icon extension",
    "icons": {
        "128": "icon_128.png"
    },
    "name": "Fastmail Checker",
    "permissions": [
        "alarms",
        "tabs",
        "webNavigation",
        "*://fastmail.com/*"
    ],
    "version": "0.0.1",
    "manifest_version": 2
}